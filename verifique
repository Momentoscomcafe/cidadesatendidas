# Função para verificar se a cidade está cadastrada no estado fornecido
def verificar_cidade(estado, cidade):
    # Converte o estado e a cidade para minúsculas para evitar diferenciação de maiúsculas/minúsculas
    estado = estado.lower()
    cidade = cidade.lower()
    
    # Verifica se o estado está na lista de estados
    if estado in cidades_por_estado:
        # Verifica se a cidade está na lista de cidades do estado fornecido
        if cidade in cidades_por_estado[estado]:
            return "Cidade não atendida. Favor entrar em contato pelo nosso WhatsApp."
        else:
            return "Cidade atendida. Peça seu creme de café hoje mesmo!"
    else:
        return "Estado não encontrado."

# Programa principal
def main():
    # Texto de instruções para o usuário
    instrucoes = "Por favor, preencha os seguintes campos:\n"
    instrucoes += "1. Estado: Digite o nome do seu estado.\n"
    instrucoes += "2. Cidade: Digite o nome da sua cidade.\n"
    print(instrucoes)
    
    # Entrada do usuário para o estado e a cidade
    estado = input("Digite o nome do estado: ")
    cidade = input("Digite o nome da cidade: ")
    
    # Verifica se a cidade está cadastrada no estado fornecido
    resultado = verificar_cidade(estado, cidade)
    
    # Exibe o resultado
    print(resultado)

# Chamada da função principal
if __name__ == "__main__":
    main()
